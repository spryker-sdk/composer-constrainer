{
    "name": "spryker-sdk/composer-constrainer",
    "type": "library",
    "description": "ComposerConstrainer module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "ondrejmirtes/better-reflection": "^4.3.0",
        "spryker/kernel": "^3.33.0",
        "spryker/laminas": "^1.0.0",
        "spryker/symfony": "^3.2.2"
    },
    "require-dev": {
        "codeception/module-asserts": "^1.3",
        "spryker/code-sniffer": "*",
        "spryker/console": "*",
        "spryker/testify": "^3.15.0",
        "phpstan/phpstan": "^0.12.0"
    },
    "autoload": {
        "psr-4": {
            "SprykerSdk\\": "src/SprykerSdk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerSdkTest\\": "tests/SprykerSdkTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "stan": "vendor/bin/phpstan analyse"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
